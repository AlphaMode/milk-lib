{
  "schemaVersion": 1,
  "id": "milk",
  "version": "${version}",
  "name": "Milk Lib",
  "description": "Literally just adds milk, to act as a bridge for any other mods that want to do the same.",
  "authors": [
    "Tropheus Jay"
  ],
  "contact": {
    "sources": "https://github.com/TropheusJ/milk-lib"
  },
  "license": "MIT",
  "icon": "assets/milk/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.tropheusj.milk.Milk"
    ],
    "client": [
      "io.github.tropheusj.milk.Client"
    ]
  },
  "mixins": [
    "milk.mixins.json"
  ],
  "accessWidener": "milk.accesswidener",
  "depends": {
    "fabricloader": ">=0.12.11",
    "fabric": ">=0.43.1",
    "minecraft": ">=1.17.1",
    "java": ">=16"
  }
}
